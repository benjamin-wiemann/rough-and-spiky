#pragma kernel TestShaderMain

RWTexture2D<float4> _RenderTexture1;
RWTexture2D<float4> _RenderTexture2;

[numthreads(8,8,1)]
void TestShaderMain (uint3 id : SV_DispatchThreadID)
{    
    _RenderTexture1[id.xy] = float4(id.x & id.y, (id.x & 15)/15.0, (id.y & 15)/15.0, 0.0);
    _RenderTexture2[id.xy] = float4(id.x & id.y, (id.x & 15)/15.0, (id.y & 15)/15.0, 0.0);
}
